@{
    ViewData["Title"] = "Bienvenido";
}

<h1>Bienvenido</h1>
<ul>
    @{
        int numVeces = (int)ViewData["numVeces"];
        string nombre_apellido = (string)ViewData["nombre_apellido"];
        for (int i = 0; i < numVeces; i++)
        {
            <li>@($"Hola {nombre_apellido}, veces mostrado = {i + 1}")</li>
        }
    }
</ul> 
